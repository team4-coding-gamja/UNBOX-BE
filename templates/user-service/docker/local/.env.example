# User Service - Local Development Environment
# Copy this file to .env and update values as needed

# Application Configuration
USER_SERVICE_PORT=8081
SPRING_PROFILES_ACTIVE=local

# Database Configuration
POSTGRES_DB=unbox_user_local
POSTGRES_USER=unbox_user
POSTGRES_PASSWORD=ChangeHERE!
POSTGRES_PORT=5433

# Redis Configuration
REDIS_PORT=6380

# JWT Configuration
SPRING_JWT_SECRET=ChangeHERE!

# Logging Configuration
LOG_LEVEL=INFO
ROOT_LOG_LEVEL=WARN

# Database Driver
DB_DRIVER_CLASS_NAME=org.postgresql.Driver